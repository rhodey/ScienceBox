BEGIN NO_OP
  WAIT 1 SECONDS
END NO_OP

BEGIN OTHER_OP
  NO_OP
  WAIT 10 SECONDS
END OTHER_OP

// procedure declaration
BEGIN SANITIZE
  TEMPERATURE 215F
  HUMIDITY     95%
  WAIT         30 MINUTES

  FAN          ON
  OTHER_OP
  WAIT          5 MINUTES
END SANITIZE

// procedure call
SANITIZE

// bounded loop
BEGIN LOOP 3
  TEMPERATURE 80C
  HUMIDITY    45%
  WAIT        10 MINUTES
  
  BEGIN LOOP 2
    TEMPERATURE 90C
    WAIT        10 MINUTES
  END LOOP
  
END LOOP

TEMPERATURE 75C
HUMIDITY    30%
WAIT         5 MINUTES